#include<iostream>
#include<vector>
using namespace std;
int FindLargestElement(vector<int> &arr,int n)
{
    int max = arr[0];
    for(int i=0;i<n;i++)
    {
        if(arr[i]>max)
        {
            max = arr[i];    // 
        }
    }
    return max;
}
int main()
{
    vector<int> arr;         // create vector array  
    int a;
    int n,largestElement;    // n for take size of an array from user
    cout<<"Enter an array size: ";
    cin>>n;
    cout<<endl;
    for(int i=0;i<n;i++)
    {
        cin>>a;
        arr.push_back(a);    // take elements of array from user
    }
    largestElement = FindLargestElement(arr,n);
    cout<<"Largest Element of array is: "<<largestElement;


}
