#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
const int a = 100;
const int b = 100;
void rotateBy90(int arr[a][b],int n)
{
    cout<<endl<<"Matrix After rotation of 90degree in anticlockwise direction: "<<endl<<endl;
    for(int i=n-1;i>=0;i--)
    {
        for(int j=0;j<n;j++)
        {
            cout<<arr[j][i]<<" ";
        }
        cout<<endl;
    }
}
int main()
{
    int arr[100][100];
    int a,n;
    
    cout<<"Enter the degree of matrix: ";
    cin>>n;
    
    cout<<"Enter the elements of matrix: ";
    cout<<endl;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>arr[i][j];
        }
    }
    rotateBy90(arr,n);
}
