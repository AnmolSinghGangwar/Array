#include<iostream>
#include<vector>
using namespace std;
void rotateByDplace(vector<int> &arr,int n,int pos)
{
    vector<int> a;
     
    for(int i=0;i<pos;i++)
        a.push_back(arr[i]);
    
    for(int j=0;j<pos;j++)
        swap(arr[j],arr[j+pos]);
    
    for(int k=pos,i=0;k<n;k++,i++)
        arr.insert(arr.begin()+k,a[i]);
    
    for(int i=0;i<n;i++)
        cout<<arr[i]<<" ";
    
}
int main()
{
    vector<int> arr;
    int a,n,pos;
    
    cout<<"Enter the size of an array: ";
    cin>>n;
    
    cout<<"Enter elemennts of an array: ";
    for(int i=0;i<n;i++)
    {
        cin>>a;
        arr.push_back(a);
    }
    
    cout<<"Enter at which place rotate in right psition: ";
    cin>>pos;
    
    rotateByDplace(arr,n,pos);
}
